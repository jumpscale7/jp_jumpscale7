param.base=/opt/pyftpserver

process.1=
    name:'$(jp.name)__$(jp.instance)',
    cmd:'python ftpstart.py',
    args:,
    prio:10,
    cwd:'$(param.base)',
    timeout_start:10,
    timeout_stop:10,
    ports:21,
    startupmanager:tmux,
    filterstr:'python ftpstart.py',

git.url='https://github.com/giampaolo/pyftpdlib'
git.export.1=
    url:$(git.url),
    source:'pyftpdlib/',
    dest:'/opt/pyftpserver/pyftpdlib/',
    link:False,
    overwrite:True
    
